{
    "version": 2,
    "builds": [
        {
            "src": "src/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/block/(.*)",
            "dest": "routes/v1/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}